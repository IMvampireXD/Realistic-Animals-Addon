{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.raa.duck.general": {
			"initial_state": "idle",
			"states": {
				"idle": {
					"animations": ["idle", "blink"],
					"transitions": [
						{
							"panic": "v.panicking_state == 'panic' && q.modified_move_speed > 0.1"
						},
						{
							"run": "v.panicking_state == 'flee' && q.modified_move_speed > 0.1"
						},
						{
							"walk": "q.modified_move_speed > 0.1"
						},
						{
							"swim": "q.is_swimming"
						},
						{
							"peck": "q.is_grazing"
						}
					],
					"blend_transition": 0.2
				},
				"walk": {
					"animations": [
						{
							"walk": "q.modified_move_speed"
						}
					],
					"transitions": [
						{
							"idle": "!(q.modified_move_speed > 0.1)"
						},
						{
							"panic": "v.panicking_state == 'panic'"
						},
						{
							"run": "v.panicking_state == 'flee'"
						},
						{
							"swim": "q.is_swimming"
						},
						{
							"peck": "q.is_grazing"
						}
					],
					"blend_transition": 0.2
				},
				"swim": {
					"animations": [
						{
							"swim": "q.modified_move_speed"
						}
					],
					"transitions": [
						{
							"panic": "v.panicking_state == 'panic' && q.modified_move_speed > 0.1"
						},
						{
							"run": "v.panicking_state == 'flee' && q.modified_move_speed > 0.1"
						},
						{
							"idle": "!q.is_swimming && !(q.modified_move_speed > 0.1)"
						},
						{
							"walk": "!q.is_swimming && q.ground_speed"
						},
						{
							"peck": "q.is_grazing"
						}
					],
					"blend_transition": 0.2
				},
				"peck": {
					"animations": [
						"peck"
					],
					"transitions": [
						{
							"idle": "!(q.modified_move_speed > 0.1)"
						},
						{
							"panic": "v.panicking_state == 'panic'"
						},
						{
							"run": "v.panicking_state == 'flee'"
						},
						{
							"swim": "q.is_swimming"
						},
						{
							"walk": "q.modified_move_speed > 0.1"
						}
					],
					"blend_transition": 0.2
				},
				"panic": {
					"animations": [
						{
							"panic": "q.modified_move_speed"
						}
					],
					"transitions": [
						{
							"idle": "!(q.modified_move_speed > 0.1)"
						},
						{
							"swim": "q.is_swimming && v.panicking_state == 'none'"
						},
						{
							"run": "q.modified_move_speed > 0.1 && v.panicking_state == 'flee'"
						},
						{
							"walk": "q.modified_move_speed > 0.1 && v.panicking_state == 'none'"
						},
						{
							"peck": "q.is_grazing && v.panicking_state == 'none'"
						}
					],
					"blend_transition": 0.2
				},
				"run": {
					"animations": [
						{
							"run": "q.modified_move_speed"
						}
					],
					"transitions": [
						{
							"idle": "!(q.modified_move_speed > 0.1)"
						},
						{
							"swim": "q.is_swimming && v.panicking_state == 'none'"
						},
						{
							"walk": "q.modified_move_speed > 0.1 && v.panicking_state == 'none'"
						},
						{
							"peck": "q.is_grazing && v.panicking_state == 'none'"
						},
						{
							"panic": "q.modified_move_speed > 0.1 && v.panicking_state == 'panic'"
						}
					],
					"blend_transition": 0.2
				}
			}
		}
	}
}