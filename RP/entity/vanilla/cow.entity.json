{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:cow",
      "min_engine_version": "1.21.0",
      "materials": { "default": "entity_alphatest" },
      "textures": {
				"black": "textures/raa/entity/cow/black_cattle",
				"brown": "textures/raa/entity/cow/brown_cattle",
				"brown_spotted": "textures/raa/entity/cow/brown_spotted_cattle",
				"cream": "textures/raa/entity/cow/cream_cattle",
				"dairy": "textures/raa/entity/cow/dairy_cattle",
				"white": "textures/raa/entity/cow/white_cattle"
      },
      "geometry": {
        "default": "geometry.raa.cow"
      },
      "animations": {
        "idle": "animation.raa.cow.idle",
        "walk": "animation.raa.cow.walk",
        "run": "animation.raa.cow.run",
        "eat": "animation.raa.cow.eat",
        "sleep": "animation.raa.cow.sleep",
        "look_at_target": "animation.raa.cow.look_at_target",
        "baby_transform": "animation.cow.baby_transform",
        "move": "controller.animation.raa.cow.move"
      },
      "scripts": {
        "pre_animation": [
          "v.variant = q.property('raa:variant');",
          "v.is_male = q.property('raa:is_male');"
        ],
        "animate": [
          "move",
          { "baby_transform": "q.is_baby" }
        ]
      },
      "particle_effects": {
        "sleep": "raa:sleep_particle"
      },
      "render_controllers": [ "controller.render.raa.cow" ],
      "spawn_egg": {
        "texture": "spawn_egg_cow"
      }
    }
  }
}