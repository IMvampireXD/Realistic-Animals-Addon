{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:pig",
      "min_engine_version": "1.21.0",
      "materials": { "default": "entity_alphatest" },
      "textures": {
				"black": "textures/raa/entity/pig/black_pig",
				"brown": "textures/raa/entity/pig/brown_pig",
				"pale": "textures/raa/entity/pig/pale_pig",
				"pink": "textures/raa/entity/pig/pink_pig",
				"splatched": "textures/raa/entity/pig/splatched_pig",
        "saddle": "textures/entity/saddle_v2"
      },
      "geometry": {
        "default": "geometry.raa.pig",
        "saddle": "geometry.raa.pig.saddle"
      },
      "animations": {
        "idle": "animation.raa.pig.idle",
        "walk": "animation.raa.pig.walk",
        "run": "animation.raa.pig.run",
        "eat": "animation.raa.pig.eat",
        "sleep": "animation.raa.pig.sleep",
        "look_at_target": "animation.raa.pig.look_at_target",
        "baby_transform": "animation.pig.baby_transform",
        "move": "controller.animation.raa.pig.move"
      },
      "scripts": {
        "pre_animation": [
          "v.variant = q.property('raa:variant');"
        ],
        "animate": [
          "move",
          { "baby_transform": "q.is_baby" }
        ]
      },
      "particle_effects": {
        "sleep": "raa:sleep_particle"
      },
      "render_controllers": [
        "controller.render.raa.pig",
        { "controller.render.raa.pig.saddle": "q.is_saddled" }
      ],
      "spawn_egg": {
        "texture": "spawn_egg_pig"
      }
    }
  }
}