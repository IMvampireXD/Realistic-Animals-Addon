{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:chicken",
      "min_engine_version": "1.21.0",
      "materials": { "default": "entity_alphatest" },
      "textures": {
				"black_hen": "textures/raa/entity/chicken/hen/black_hen",
				"brown_hen": "textures/raa/entity/chicken/hen/brown_hen",
				"white_hen": "textures/raa/entity/chicken/hen/white_hen",
				"brown_rooster": "textures/raa/entity/chicken/rooster/brown_rooster",
				"perdiz_rooster": "textures/raa/entity/chicken/rooster/perdiz_rooster",
				"white_rooster": "textures/raa/entity/chicken/rooster/white_rooster",
				"chick": "textures/raa/entity/chicken/chick"
      },
      "geometry": {
        "default": "geometry.raa.chicken"
      },
      "animations": {
        "idle": "animation.raa.chicken.idle",
        "walk": "animation.raa.chicken.walk",
        "run": "animation.raa.chicken.run",
        "fly": "animation.raa.chicken.fly",
        "eat": "animation.raa.chicken.eat",
        "sleep": "animation.raa.chicken.sleep",
        "look_at_target": "animation.raa.chicken.look_at_target",
        "baby_transform": "animation.chicken.baby_transform",
        "move": "controller.animation.raa.chicken.move"
      },
      "scripts": {
        "pre_animation": [
          "v.variant = q.property('raa:variant');",
          "v.is_male = q.property('raa:is_male');",
          "v.panicking_state = q.property('raa:panicking_state');"
        ],
        "animate": [
          "move",
          { "baby_transform": "q.is_baby" }
        ]
      },
      "particle_effects": {
        "sleep": "raa:sleep_particle"
      },
      "render_controllers": [ "controller.render.raa.chicken" ],
      "spawn_egg": {
        "texture": "spawn_egg_chicken"
      }
    }
  }
}