{
  "format_version": "1.21.90",
  "minecraft:entity": {
    "description": {
      "identifier": "raa:egg",
      "is_spawnable": false,
      "is_summonable": true,
      "is_experimental": false
    },
    "component_groups": {
      "egg": {
        "minecraft:mark_variant": {
          "value": 0
        }
      },
      "blue_egg": {
        "minecraft:mark_variant": {
          "value": 1
        }
      },
      "brown_egg": {
        "minecraft:mark_variant": {
          "value": 2
        }
      }
    },
    "components": {
      "minecraft:type_family": {
        "family": [ "inanimate" ]
      },
      "minecraft:health": {
        "value": 1,
        "max": 1
      },
      "minecraft:physics": {
        "has_gravity": true,
        "has_collision": true
      },
      "minecraft:collision_box": {
        "width": 0.1,
        "height": 0.1
      },
      "minecraft:custom_hit_test": {
        "hitboxes": [
          {
            "pivot": [
              0,
              0,
              0
            ],
            "width": 0.2,
            "height": 0.4
          }
        ]
      },
      "minecraft:damage_sensor": {
        "triggers": {
          "cause": "all",
          "deals_damage": "no"
        }
      },
      "minecraft:cannot_be_attacked": {
      },
      "minecraft:conditional_bandwidth_optimization": {
      },
      "minecraft:entity_sensor": {
        "relative_range": true,
        "subsensors": [
          {
            "cooldown": 0.1,
            "range": [ 0.25, 0.25 ],
            "minimum_count": 1,
            "require_all": false,
            "event_filters": [
              { "test": "is_moving", "subject": "other", "value": true },
              { "test": "on_ground", "value": true }
            ],
            "event": "raa:egg_sway"
          }
        ]
      }
    },
    "events": {
      "raa:blue_egg": {
        "add": {
          "component_groups": [
            "blue_egg"
          ]
        }
      },
      "raa:brown_egg": {
        "add": {
          "component_groups": [
            "brown_egg"
          ]
        }
      },
      "raa:egg_sway": {
        "queue_command": {
          "command": "playanimation @s animation.raa.egg.sway"
        }
      }
    }
  }
}